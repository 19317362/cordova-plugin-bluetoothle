<?xml version='1.0' encoding='utf-8'?>
<widget id="com.randdusing.bluetoothleexample" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="BluetoothLePlugin">
        <param name="android-package" value="com.randdusing.bluetoothle.BluetoothLePlugin" />
    </feature>
    <name>BluetoothLeExample</name>
    <description>
        A sample application to demonstrate the Bluetooth LE plugin
    </description>
    <author email="{firstNamelastName}@gmail.com" href="http://www.randdusing.com">
        Rand Dusing
    </author>
    <preference name="fullscreen" value="true" />
    <preference name="KeepRunning" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <content src="index.html" />
    <access origin="*" />
</widget>
